{
  "name": "Cyclic Visualizer",
  "version": "0.0.1",
  "description": "Cyclejs Stream & Component Visualizer",
  "devtools_page": "devtoolsPage.html",
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "web_accessible_resources": [ "pageScript.js", "panel.js", "contentScript.js" ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "minimum_chrome_version": "22.0",
  "permissions": [ "tabs", "contextMenus", "<all_urls>" ],
  "manifest_version": 2
}
