{
  "name": "cyclic-visualizer",
  "version": "0.0.1",
  "description": "Cyclejs Stream & Component Visualizer",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "node_modules/.bin/tslint -c tslint.json .src/*.ts .src/**/*.ts",
    "cleanDist": "rm -rf dist/*",
    "copyAssets": "cp -r assets/* dist/",
    "compile": "node_modules/.bin/tsc",
    "distChromeBackground": "node_modules/.bin/browserify build/chrome-extension/background.js -s background --outfile dist/chrome-extension/background.js",
    "distChromeDevtoolsPage": "node_modules/.bin/browserify build/chrome-extension/devtoolsPage.js -s devtoolsPage --outfile dist/chrome-extension/devtoolsPage.js",
    "distChromePanel": "node_modules/.bin/browserify build/chrome-extension/panel.js -s panel --outfile dist/chrome-extension/panel.js",
    "distChromeContentScript": "node_modules/.bin/browserify build/chrome-extension/contentScript.js -s contentScript --outfile dist/chrome-extension/contentScript.js",
    "distChromePageScript": "node_modules/.bin/browserify build/chrome-extension/pageScript.js -s pageScript --outfile dist/chrome-extension/pageScript.js",
    "distChrome": "npm run distChromeBackground && npm run distChromeDevtoolsPage && npm run distChromeContentScript && npm run distChromePageScript && npm run distChromePanel",
    "build": "npm run cleanDist && npm run copyAssets && npm run lint && npm run compile && npm run distChrome"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/siassaj/cyclic-visualizer.git"
  },
  "author": "siavash sajjadi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/siassaj/cyclic-visualizer/issues"
  },
  "homepage": "https://github.com/siassaj/cyclic-visualizer#readme",
  "dependencies": {
    "@types/chrome": "^0.0.76",
    "browserify": "^16.2.3",
    "xstream": "^11.7.0"
  },
  "devDependencies": {
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  }
}
